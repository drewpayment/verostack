  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build": "ng build --configuration=development",
    "build:watch": "npm-watch build:development",
    "build:prod": "ng build --prod",
    "postbuild": "node ./prod-build.js",
